{
  "$schema": "node_modules/wrangler/config-schema.json",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  "compatibility_date": "2025-10-17",
  "name": "my-app-web",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "preview_urls": true,
  "routes": [
    {
      "custom_domain": true,
      "pattern": "app.example.com"
    }
  ],
  "send_metrics": false
}
